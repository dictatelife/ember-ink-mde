{
  "compilerOptions": {
    "target": "ES2021",
    "module": "ES2020",
    "allowJs": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noEmitOnError": false,
    "noEmit": true,
    "inlineSourceMap": true,
    "inlineSources": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "dummy/tests/*": ["tests/*"],
      "dummy/*": ["tests/dummy/app/*", "app/*"],
      "ember-ink-mde": ["addon"],
      "ember-ink-mde/*": ["addon/*"],
      "ember-ink-mde/test-support": ["addon-test-support"],
      "ember-ink-mde/test-support/*": ["addon-test-support/*"],
      "*": ["types/*"]
    }
  },
"include": [
  "app/**/*",
  "addon/**/*",
  "tests/**/*",
  "types/**/*",
  "test-support/**/*",
  "addon-test-support/**/*"
]
}
